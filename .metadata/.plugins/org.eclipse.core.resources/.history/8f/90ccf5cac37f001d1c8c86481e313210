  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
#endif
#include <stdint.h>

int main(void)
{
    uint32_t *pRCC = (uint32_t*)0x40023830;
    uint32_t *pModeAReg = (uint32_t*)0x40020000;
    uint32_t *pOutAReg = (uint32_t*)0x40020014;

    //enabling Clock for GPIOA
    *pRCC |= (1 << 0);

    //OUtput mode configuration for GPIOA
    *pModeAReg &= ~(3 << 10);	//clear the 10th and 11th bit position
    *pModeAReg |= (1 << 10);	//set the 10th bit position to 1

    while(1){
		//set the 5th bit of output data register to make I/O pin high
		*pOutAReg |= (1 << 5);

		for(uint32_t i = 0; i < 10000; i++);

		*pOutAReg &= ~(1 << 5);

		for(uint32_t i = 0; i < 10000; i++);

    }

}
